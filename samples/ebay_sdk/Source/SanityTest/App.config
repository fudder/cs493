<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="devid" value=""/>
    <add key="appid" value=""/>
    <add key="cert" value=""/>
    <!-- token for both API server and EPS server -->
    <add key="token" value=""/>
    <!-- for production use: https://api.ebay.com/ws/api.dll -->
    <add key="epsurl" value="https://api.sandbox.ebay.com/ws/api.dll"/>

    <!-- For production site use: https://api.ebay.com/wsapi -->
    <!-- For Sandbox use: https://api.sandbox.ebay.com/wsapi -->    
    
    <add key="soapurl" value="https://api.sandbox.ebay.com/wsapi"/>
    <add key="timeout" value="90000"/>
    <add key="logexception" value="true"/>
    <add key="logmessages" value="true"/>
    <add key="logsdkmessages" value="true"/>
    <add key="logsdk" value="true"/>
    <add key="logfile" value="Log.txt"/>
